    "keywords": ["neo4j","graph","database","entity-mapper","doctrine"],
    "homepage": "https://github.com/lphuberdeau/Neo4j-PHP-OGM",
    "license": "MIT",
    "authors": [
        {"name": "Louis-Philippe Huberdeau", "email": "lphuberdeau@gmail.com", "homepage": "http://lphuberdeau.com"},
        {"name": "Thomas Alatal Berg", "email": "thomas@izytech.se", "homepage": "http://izytech.ses"}
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": ">=2.2.0",
        "everyman/neo4jphp": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "izytechAB": "lib/"
